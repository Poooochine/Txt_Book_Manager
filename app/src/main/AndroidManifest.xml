<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.txt_book_manager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Txt_Book_Manager">
        <activity
            android:name=".add"
            android:exported="false"
            android:label="@string/title_activity_add"
            android:theme="@style/Theme.Txt_Book_Manager.NoActionBar" />
        <activity
            android:name=".Add"
            android:exported="false"
            android:label="@string/title_activity_add"
            android:theme="@style/Theme.Txt_Book_Manager.NoActionBar" />
        <activity
            android:name=".uladapter"
            android:exported="false"
            android:label="@string/title_activity_uladapter"
            android:theme="@style/Theme.Txt_Book_Manager.NoActionBar" />
        <activity
            android:name=".book_listing"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Txt_Book_Manager.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>